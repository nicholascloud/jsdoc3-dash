{
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true,
    "browser": true,
    "mocha": true
  },
  "globals": {
    "desc": true,
    "task": true,
    "fail": true,
    "complete": true,
    "namespace": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": [2, "last"],
    "curly": [2, "all"],
    "dot-notation": [2, {"allowKeywords": true}],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-with": 2,
    "quotes": [2, "single"],
    "semi": [0, "never"],
    "strict": 0,
    "valid-typeof": 2,
    "wrap-iife": [2, "outside"]
  }
}